{
  "vant-toast": {
    "prefix": "toast",
    "body": [
      "this.\\$toast({",
      "\tmessage: '${1:Content}',",
      "})"
    ],
    "description": "toast"
  },
  "vant-dialog": {
    "prefix": "dialog",
    "body": [
      "this.\\$dialog.alert({",
      "\tmessage: '${1:Content}',",
      "})"
    ],
    "description": "alert"
  },
  "vant-notify": {
    "prefix": "notify",
    "body": [
      "this.\\$notify({",
      "\ttype: '${1:primary}',",
      "\tmessage: '${2:Content}',",
      "})"
    ],
    "description": "notify"
  }
}
